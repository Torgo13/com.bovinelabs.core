<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" editor-extension-mode="True">
    <ui:Button text="Extensions Disabled" display-tooltip-when-elided="true" name="EnableExtensions" class="enable-extensions main-button" />
    <ui:ScrollView class="feature-view">
        <ui:VisualElement tooltip="Adds an automatic way to load roslyn analyzers on all projects." class="feature-group">
            <ui:Label tabindex="-1" text="Analyzers" display-tooltip-when-elided="true" name="BL_DISABLE_ANALYZERS" />
            <ui:Button text="Enabled" display-tooltip-when-elided="true" class="feature-button" />
        </ui:VisualElement>
        <ui:VisualElement tooltip="Adds an Entropy singleton that can be queried for thread safe random." class="feature-group">
            <ui:Label tabindex="-1" text="Entropy" display-tooltip-when-elided="true" name="BL_DISABLE_ENTROPY" />
            <ui:Button text="Enabled" display-tooltip-when-elided="true" class="feature-button" />
        </ui:VisualElement>
        <ui:VisualElement tooltip="Adds an InputCommon component you can query for common input info such as InputOverUI, CameraRay, ScreenPoint and ViewPoint." class="feature-group">
            <ui:Label tabindex="-1" text="Input" display-tooltip-when-elided="true" name="BL_DISABLE_INPUT" />
            <ui:Button text="Enabled" display-tooltip-when-elided="true" class="feature-button" />
        </ui:VisualElement>
        <ui:VisualElement tooltip="Adds a custom, flexible and safe Entity Destroy pipeline and structural change." class="feature-group">
            <ui:Label tabindex="-1" text="Life Cycle" display-tooltip-when-elided="true" name="BL_DISABLE_LIFECYCLE" />
            <ui:Button text="Enabled" display-tooltip-when-elided="true" class="feature-button" />
        </ui:VisualElement>
        <ui:VisualElement tooltip="Adds an auto managed object identification system that is git merge safe." class="feature-group">
            <ui:Label tabindex="-1" text="Object Definitions" display-tooltip-when-elided="true" name="BL_DISABLE_OBJECT_DEFINITION" />
            <ui:Button text="Enabled" display-tooltip-when-elided="true" class="feature-button" />
        </ui:VisualElement>
        <ui:VisualElement tooltip="A more performant parallel version of Unity&apos;s physics states." class="feature-group">
            <ui:Label tabindex="-1" text="Physics States" display-tooltip-when-elided="true" name="BL_DISABLE_PHYSICS_STATES" />
            <ui:Button text="Enabled" display-tooltip-when-elided="true" class="feature-button" />
        </ui:VisualElement>
        <ui:VisualElement tooltip="Adds support for ensuring certain subscenes have loaded before the simulation starts running. Also provides a volume based runtime scene loading and a general purpose Pause system." class="feature-group">
            <ui:Label tabindex="-1" text="Subscene Loading" display-tooltip-when-elided="true" name="BL_DISABLE_SUBSCENE" />
            <ui:Button text="Enabled" display-tooltip-when-elided="true" class="feature-button" />
        </ui:VisualElement>
        <ui:VisualElement tooltip="Adds a new UnityTime singleton that provides access to various time properties such as FrameCount, UnscaleTime etc" class="feature-group">
            <ui:Label tabindex="-1" text="Time" display-tooltip-when-elided="true" name="BL_DISABLE_TIME" />
            <ui:Button text="Enabled" display-tooltip-when-elided="true" class="feature-button" />
        </ui:VisualElement>
        <ui:VisualElement tooltip="Adds flexible debug toolbar panel. Requires UI to be enabled." class="feature-group">
            <ui:Label tabindex="-1" text="Toolbar" display-tooltip-when-elided="true" name="BL_DISABLE_TOOLBAR" />
            <ui:Button text="Enabled" display-tooltip-when-elided="true" class="feature-button" />
        </ui:VisualElement>
        <ui:VisualElement tooltip="Adds a UI framework for managing UI Toolkit with entities." class="feature-group">
            <ui:Label tabindex="-1" text="UI System" display-tooltip-when-elided="true" name="BL_DISABLE_UI" />
            <ui:Button text="Enabled" display-tooltip-when-elided="true" class="feature-button" />
        </ui:VisualElement>
    </ui:ScrollView>
    <ui:Button text="Apply" display-tooltip-when-elided="true" name="ApplyChanges" class="main-button" />
</ui:UXML>
